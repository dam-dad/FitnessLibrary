<ScrollPane fx:id="LeftMenuView" fitToHeight="true" hbarPolicy="NEVER" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1">
   <content>
      <VBox alignment="TOP_CENTER" spacing="5.0">
         <children>
            <TitledPane alignment="TOP_LEFT" text="Equipment" VBox.vgrow="ALWAYS">
               <VBox.margin>
                  <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
               </VBox.margin>
               <content>
                  <VBox fx:id="equipamientoVBox" prefHeight="200.0" prefWidth="150.0" spacing="5.0" style="-fx-font-weight: normal;">
                     <padding>
                        <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
                     </padding>
                     <opaqueInsets>
                        <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
                     </opaqueInsets>
                  </VBox>
               </content>
               <font>
                  <Font name="System Bold" size="12.0" />
               </font>
            </TitledPane>
            <TitledPane alignment="TOP_LEFT" style="-fx-font-weight: bold;" text="Muscular Group" VBox.vgrow="ALWAYS">
               <VBox.margin>
                  <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
               </VBox.margin>
               <content>
                  <VBox fx:id="grupoMuscularVBox" prefHeight="200.0" prefWidth="150.0" spacing="5.0" style="-fx-font-weight: normal;">
