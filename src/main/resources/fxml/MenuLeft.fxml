                  </VBox>
               </content>
            </TitledPane>
            <TitledPane alignment="TOP_LEFT" style="-fx-font-weight: bold;" text="Muscle" VBox.vgrow="ALWAYS">
               <VBox.margin>
                  <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
               </VBox.margin>
               <content>
                  <VBox fx:id="musculoVBox" prefHeight="200.0" prefWidth="150.0" spacing="5.0" style="-fx-font-weight: normal;">
                     <padding>
                        <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
                     </padding>
                     <opaqueInsets>
                        <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
