      <RowConstraints minHeight="10.0" vgrow="SOMETIMES" />
   </rowConstraints>
   <children>
      <Pane fx:id="MainIMGPane">
         <children>
            <ImageView fx:id="imageViewMain" opacity="0.0" pickOnBounds="true">
               <image>
                  <Image url="@../img/WhatsApp%20Image%202022-02-21%20at%203.26.41%20PM.jpeg" />
               </image>
               <viewport>
                  <Rectangle2D />
               </viewport>
            </ImageView>
            <Button fx:id="btn_Start" layoutX="344.0" layoutY="481.0" mnemonicParsing="false" onAction="#onClickStart" prefHeight="41.0" prefWidth="103.0" text="Start" />
            <Button fx:id="btn_Exit" layoutX="673.0" layoutY="540.0" mnemonicParsing="false" onAction="#onClickExit" text="Exit" />
         </children>
      </Pane>
   </children>
